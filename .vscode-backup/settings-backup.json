{
	"workbench.colorTheme": "One Dark Pro Mix",
	"workbench.iconTheme": "material-icon-theme",
	"editor.fontLigatures": true,
	"editor.fontFamily": "Victor Mono , Menlo, Monaco, 'Courier New', monospace",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"explorer.confirmDragAndDrop": false,
	"cSpell.userWords": [
		"Hola",
		"ingestor",
		"nexstar",
		"nextjs",
		"rivian",
		"Rivian",
		"tailwindcss",
		"Tesseract",
		"testid",
		"tsseract",
		"unoptimized"
	],
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"javascript.updateImportsOnFileMove.enabled": "always",
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": ["<S-j>"],
			"after": ["1", "0", "j"]
		},
		{
			"before": ["<S-k>"],
			"after": ["1", "0", "k"]
		}
	],
	"vim.useSystemClipboard": true,
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter-notebook"
	},
	"notebook.cellToolbarLocation": {
		"default": "right",
		"jupyter-notebook": "left"
	},
	"window.restoreFullscreen": true,
	"window.restoreWindows": "none",
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"python.defaultInterpreterPath": "/opt/homebrew/bin/python3",
	"prettier.useTabs": true,
	"editor.suggestSelection": "first",
	"cSpell.language": "en,es",
	"typescript.updateImportsOnFileMove.enabled": "always",
	"editor.bracketPairColorization.enabled": true,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"files.exclude": {
		"**/.classpath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/.factorypath": true
	},
	"files.associations": {
		"*.tsx": "typescriptreact"
	},
	"workbench.sideBar.location": "right",
	"diffEditor.ignoreTrimWhitespace": false,
	"editor.guides.bracketPairs": "active",
	"editor.cursorBlinking": "phase",
	"editor.cursorSmoothCaretAnimation": "on",
	"thunder-client.codeSnippetLanguage": "cs-httpclient",
	"editor.largeFileOptimizations": false,
	"phpcs.enable": true,
	"phpcs.standard": "~/dev/nxs/phpcs.xml",
	"phpcs.ignorePatterns": ["*/vendor/*"],
	"phpcs.executablePath": "/Users/jem/.composer/vendor/bin/phpcs",
	"phpcbf.executablePath": "/Users/jem/.composer/vendor/bin/phpcbf",
	"phpcbf.standard": "~/dev/nxs/phpcs.xml",
	"editor.linkedEditing": true,
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": false,
		"typescriptreact": true
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"window.zoomLevel": 1,
	"liveServer.settings.donotShowInfoMsg": true
}
